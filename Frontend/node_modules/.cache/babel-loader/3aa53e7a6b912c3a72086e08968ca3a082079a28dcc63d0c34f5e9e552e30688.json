{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'RecordEdit',\n  props: {\n    id: {\n      type: String,\n      required: true\n    }\n  },\n  data() {\n    return {\n      isNewRecord: false,\n      record: {\n        record_name: '',\n        inquiry_time: '',\n        inquiry_address: '',\n        assistant_name: '',\n        inquirer_name: '',\n        inquiry_id: '',\n        video_file_path: '',\n        text_content: '',\n        content_hash: ''\n      },\n      selectedCaseId: '',\n      availableCases: [],\n      formErrors: []\n    };\n  },\n  computed: {\n    wordCount() {\n      return this.record.text_content ? this.record.text_content.length : 0;\n    }\n  },\n  created() {\n    this.isNewRecord = this.id === 'new';\n    if (!this.isNewRecord) {\n      this.fetchRecordData();\n    }\n    this.fetchAvailableCases();\n  },\n  methods: {\n    fetchRecordData() {\n      // 模拟从API获取笔录数据\n      setTimeout(() => {\n        this.record = {\n          id: this.id,\n          record_name: '张三询问笔录',\n          inquiry_time: '2023-06-15T14:30',\n          inquiry_address: '北京市海淀区公安局询问室',\n          assistant_name: '李警官',\n          inquirer_name: '张三',\n          inquiry_id: '110101199001011234',\n          video_file_path: '/videos/record_123.mp4',\n          text_content: '问：请问你的基本情况？\\n答：我叫张三，今年35岁，住在北京市海淀区。\\n问：请问你昨天晚上在哪里？\\n答：我昨天晚上在家里看电视。',\n          content_hash: '8f7d56a...',\n          case_id: '1'\n        };\n        this.selectedCaseId = this.record.case_id || '';\n      }, 500);\n    },\n    fetchAvailableCases() {\n      // 模拟从API获取可用案件列表\n      setTimeout(() => {\n        this.availableCases = [{\n          id: '1',\n          case_name: '张三盗窃案',\n          case_number: 'A2023001'\n        }, {\n          id: '2',\n          case_name: '李四诈骗案',\n          case_number: 'A2023002'\n        }, {\n          id: '3',\n          case_name: '王五伤害案',\n          case_number: 'A2023003'\n        }];\n      }, 300);\n    },\n    validateForm() {\n      this.formErrors = [];\n      if (!this.record.record_name) {\n        this.formErrors.push('笔录名称不能为空');\n      }\n      if (!this.record.inquiry_time) {\n        this.formErrors.push('询问时间不能为空');\n      }\n      if (!this.record.inquiry_address) {\n        this.formErrors.push('询问地点不能为空');\n      }\n      if (!this.record.assistant_name) {\n        this.formErrors.push('询问员不能为空');\n      }\n      if (!this.record.inquirer_name) {\n        this.formErrors.push('被询问人不能为空');\n      }\n      if (!this.record.text_content) {\n        this.formErrors.push('笔录内容不能为空');\n      }\n      return this.formErrors.length === 0;\n    },\n    saveRecord() {\n      if (!this.validateForm()) {\n        // 滚动到表单顶部显示错误信息\n        window.scrollTo(0, 0);\n        return;\n      }\n\n      // 准备提交的数据\n      const recordData = {\n        ...this.record,\n        case_id: this.selectedCaseId || null\n      };\n\n      // 模拟API保存\n      console.log('保存笔录数据:', recordData);\n\n      // 显示成功消息并返回\n      this.$toast.success('笔录保存成功');\n      this.goBack();\n    },\n    insertTemplate(template) {\n      this.record.text_content += template;\n      // 聚焦文本区域\n      this.$nextTick(() => {\n        document.getElementById('textContent').focus();\n      });\n    },\n    goBack() {\n      this.$router.go(-1);\n    }\n  }\n};","map":{"version":3,"names":["name","props","id","type","String","required","data","isNewRecord","record","record_name","inquiry_time","inquiry_address","assistant_name","inquirer_name","inquiry_id","video_file_path","text_content","content_hash","selectedCaseId","availableCases","formErrors","computed","wordCount","length","created","fetchRecordData","fetchAvailableCases","methods","setTimeout","case_id","case_name","case_number","validateForm","push","saveRecord","window","scrollTo","recordData","console","log","$toast","success","goBack","insertTemplate","template","$nextTick","document","getElementById","focus","$router","go"],"sources":["/Users/sunziang/IdeaProjects/backend_bilu/bilu_backend2/src/views/RecordEdit.vue"],"sourcesContent":["<template>\n  <div class=\"container-fluid py-4\">\n    <div class=\"row justify-content-center\">\n      <div class=\"col-lg-10 col-md-11\">\n        <!-- 页面标题 -->\n        <div class=\"d-flex justify-content-between align-items-center mb-4\">\n          <h2 class=\"fw-bold text-primary\">\n            <i class=\"bi bi-pencil-square me-2\"></i> {{ isNewRecord ? '创建笔录' : '编辑笔录' }}\n          </h2>\n          <button @click=\"goBack\" class=\"btn btn-light rounded-pill shadow-sm px-4\">\n            <i class=\"bi bi-arrow-left me-1\"></i> 返回\n          </button>\n        </div>\n        \n        <!-- 加密状态提示 -->\n        <div v-if=\"record.content_hash\" class=\"alert alert-warning border-0 shadow-sm mb-4\">\n          <div class=\"d-flex align-items-center\">\n            <div class=\"bg-warning bg-opacity-25 p-2 rounded-circle me-3\">\n              <i class=\"bi bi-shield-lock-fill text-warning fs-4\"></i>\n            </div>\n            <div>\n              <h5 class=\"mb-1\">此笔录已加密保护</h5>\n              <p class=\"mb-0\">修改笔录内容将导致哈希值变化，需要重新验证。请谨慎操作！</p>\n            </div>\n          </div>\n        </div>\n        \n        <!-- 编辑表单 -->\n        <div class=\"card border-0 shadow-sm rounded-3 overflow-hidden\">\n          <div class=\"card-header bg-primary bg-opacity-10 text-primary fw-bold border-bottom\">\n            <i class=\"bi bi-file-text me-2\"></i> 笔录信息\n          </div>\n          <div class=\"card-body p-4\">\n            <form @submit.prevent=\"saveRecord\">\n              <!-- 表单错误提示 -->\n              <div v-if=\"formErrors.length > 0\" class=\"alert alert-danger border-0 shadow-sm mb-4\">\n                <div class=\"d-flex\">\n                  <div class=\"me-3\">\n                    <i class=\"bi bi-exclamation-triangle-fill fs-3 text-danger\"></i>\n                  </div>\n                  <div>\n                    <h5 class=\"mb-2\">表单验证错误</h5>\n                    <ul class=\"mb-0 ps-3\">\n                      <li v-for=\"(error, index) in formErrors\" :key=\"index\">{{ error }}</li>\n                    </ul>\n                  </div>\n                </div>\n              </div>\n              \n              <!-- 基本信息 -->\n              <div class=\"row mb-4\">\n                <div class=\"col-md-6 mb-3\">\n                  <label for=\"recordName\" class=\"form-label fw-medium\">笔录名称 <span class=\"text-danger\">*</span></label>\n                  <div class=\"input-group shadow-sm rounded-3 overflow-hidden\">\n                    <span class=\"input-group-text bg-light border-0\">\n                      <i class=\"bi bi-file-earmark-text text-primary\"></i>\n                    </span>\n                    <input \n                      type=\"text\" \n                      class=\"form-control border-0\" \n                      id=\"recordName\" \n                      v-model=\"record.record_name\" \n                      placeholder=\"请输入笔录名称\"\n                      required\n                    >\n                  </div>\n                </div>\n                <div class=\"col-md-6 mb-3\">\n                  <label for=\"inquiryTime\" class=\"form-label fw-medium\">询问时间 <span class=\"text-danger\">*</span></label>\n                  <div class=\"input-group shadow-sm rounded-3 overflow-hidden\">\n                    <span class=\"input-group-text bg-light border-0\">\n                      <i class=\"bi bi-calendar-event text-primary\"></i>\n                    </span>\n                    <input \n                      type=\"datetime-local\" \n                      class=\"form-control border-0\" \n                      id=\"inquiryTime\" \n                      v-model=\"record.inquiry_time\"\n                      required\n                    >\n                  </div>\n                </div>\n                <div class=\"col-md-6 mb-3\">\n                  <label for=\"inquiryAddress\" class=\"form-label fw-medium\">询问地点 <span class=\"text-danger\">*</span></label>\n                  <div class=\"input-group shadow-sm rounded-3 overflow-hidden\">\n                    <span class=\"input-group-text bg-light border-0\">\n                      <i class=\"bi bi-geo-alt text-primary\"></i>\n                    </span>\n                    <input \n                      type=\"text\" \n                      class=\"form-control border-0\" \n                      id=\"inquiryAddress\" \n                      v-model=\"record.inquiry_address\" \n                      placeholder=\"请输入询问地点\"\n                      required\n                    >\n                  </div>\n                </div>\n                <div class=\"col-md-6 mb-3\">\n                  <label for=\"assistantName\" class=\"form-label fw-medium\">询问员 <span class=\"text-danger\">*</span></label>\n                  <div class=\"input-group shadow-sm rounded-3 overflow-hidden\">\n                    <span class=\"input-group-text bg-light border-0\">\n                      <i class=\"bi bi-person text-primary\"></i>\n                    </span>\n                    <input \n                      type=\"text\" \n                      class=\"form-control border-0\" \n                      id=\"assistantName\" \n                      v-model=\"record.assistant_name\" \n                      placeholder=\"请输入询问员姓名\"\n                      required\n                    >\n                  </div>\n                </div>\n                <div class=\"col-md-6 mb-3\">\n                  <label for=\"inquirerName\" class=\"form-label fw-medium\">被询问人 <span class=\"text-danger\">*</span></label>\n                  <div class=\"input-group shadow-sm rounded-3 overflow-hidden\">\n                    <span class=\"input-group-text bg-light border-0\">\n                      <i class=\"bi bi-person-circle text-primary\"></i>\n                    </span>\n                    <input \n                      type=\"text\" \n                      class=\"form-control border-0\" \n                      id=\"inquirerName\" \n                      v-model=\"record.inquirer_name\" \n                      placeholder=\"请输入被询问人姓名\"\n                      required\n                    >\n                  </div>\n                </div>\n                <div class=\"col-md-6 mb-3\">\n                  <label for=\"inquiryId\" class=\"form-label fw-medium\">身份证号</label>\n                  <div class=\"input-group shadow-sm rounded-3 overflow-hidden\">\n                    <span class=\"input-group-text bg-light border-0\">\n                      <i class=\"bi bi-credit-card text-primary\"></i>\n                    </span>\n                    <input \n                      type=\"text\" \n                      class=\"form-control border-0\" \n                      id=\"inquiryId\" \n                      v-model=\"record.inquiry_id\" \n                      placeholder=\"请输入被询问人身份证号\"\n                    >\n                  </div>\n                </div>\n              </div>\n              \n              <!-- 案件信息 -->\n              <div class=\"mb-4\">\n                <h5 class=\"fw-bold mb-3\">\n                  <i class=\"bi bi-folder me-2 text-primary\"></i> 案件信息\n                </h5>\n                <div class=\"row\">\n                  <div class=\"col-md-6 mb-3\">\n                    <label for=\"caseSelect\" class=\"form-label fw-medium\">关联案件</label>\n                    <div class=\"input-group shadow-sm rounded-3 overflow-hidden\">\n                      <span class=\"input-group-text bg-light border-0\">\n                        <i class=\"bi bi-folder2-open text-primary\"></i>\n                      </span>\n                      <select \n                        class=\"form-select border-0\" \n                        id=\"caseSelect\" \n                        v-model=\"selectedCaseId\"\n                      >\n                        <option value=\"\">-- 不关联案件 --</option>\n                        <option v-for=\"caseItem in availableCases\" :key=\"caseItem.id\" :value=\"caseItem.id\">\n                          {{ caseItem.case_name }} ({{ caseItem.case_number }})\n                        </option>\n                      </select>\n                    </div>\n                  </div>\n                  <div class=\"col-md-6 mb-3\">\n                    <label for=\"videoFilePath\" class=\"form-label fw-medium\">视频文件路径</label>\n                    <div class=\"input-group shadow-sm rounded-3 overflow-hidden\">\n                      <span class=\"input-group-text bg-light border-0\">\n                        <i class=\"bi bi-camera-video text-primary\"></i>\n                      </span>\n                      <input \n                        type=\"text\" \n                        class=\"form-control border-0\" \n                        id=\"videoFilePath\" \n                        v-model=\"record.video_file_path\" \n                        placeholder=\"请输入视频文件路径\"\n                      >\n                    </div>\n                  </div>\n                </div>\n              </div>\n              \n              <!-- 笔录内容 -->\n              <div class=\"mb-4\">\n                <label for=\"textContent\" class=\"form-label fw-medium\">笔录内容 <span class=\"text-danger\">*</span></label>\n                <div class=\"shadow-sm rounded-3 overflow-hidden\">\n                  <div class=\"bg-light p-2 border-bottom d-flex justify-content-between align-items-center\">\n                    <div>\n                      <button type=\"button\" class=\"btn btn-sm btn-outline-secondary me-1\" @click=\"insertTemplate('问：')\">\n                        <i class=\"bi bi-chat-left-text me-1\"></i>问\n                      </button>\n                      <button type=\"button\" class=\"btn btn-sm btn-outline-secondary me-1\" @click=\"insertTemplate('答：')\">\n                        <i class=\"bi bi-chat-right-text me-1\"></i>答\n                      </button>\n                    </div>\n                    <div>\n                      <small class=\"text-muted\">字数：{{ wordCount }}</small>\n                    </div>\n                  </div>\n                  <textarea \n                    class=\"form-control border-0\" \n                    id=\"textContent\" \n                    v-model=\"record.text_content\" \n                    rows=\"15\" \n                    placeholder=\"请输入笔录内容，建议使用'问：'和'答：'的格式\"\n                    required\n                  ></textarea>\n                </div>\n              </div>\n              \n              <!-- 操作按钮 -->\n              <div class=\"d-flex justify-content-end mt-4\">\n                <button type=\"button\" @click=\"goBack\" class=\"btn btn-light rounded-pill me-2 px-4\">\n                  <i class=\"bi bi-x-circle me-1\"></i> 取消\n                </button>\n                <button type=\"submit\" class=\"btn btn-primary rounded-pill px-4\">\n                  <i class=\"bi bi-check-circle me-1\"></i> 保存修改\n                </button>\n              </div>\n            </form>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'RecordEdit',\n  props: {\n    id: {\n      type: String,\n      required: true\n    }\n  },\n  data() {\n    return {\n      isNewRecord: false,\n      record: {\n        record_name: '',\n        inquiry_time: '',\n        inquiry_address: '',\n        assistant_name: '',\n        inquirer_name: '',\n        inquiry_id: '',\n        video_file_path: '',\n        text_content: '',\n        content_hash: ''\n      },\n      selectedCaseId: '',\n      availableCases: [],\n      formErrors: []\n    }\n  },\n  computed: {\n    wordCount() {\n      return this.record.text_content ? this.record.text_content.length : 0\n    }\n  },\n  created() {\n    this.isNewRecord = this.id === 'new'\n    if (!this.isNewRecord) {\n